{
  "hosting": {
    "rewrites": [ {
      "source": "**",
      "function": "app"
    } ]
  },
  "functions": [
    {
      "source": ".",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "runtime": "nodejs20"
    }
  ]
}
